{
   "name": "dboun",
   "version": "1.0.2",
   "description": "A utility to debounce functions for better performance and control.",
   "main": "./index.js",
   "module": "./esm/index.js",
   "types": "./types/index.d.ts",
   "exports": {
      ".": {
         "types": "./types/index.d.ts",
         "import": "./index.js",
         "require": "./cjs/index.js"
      },
      "./types/*": "./types/*.d.ts",
      "./cjs/*": "./cjs/*.js",
      "./*": {
         "import": "./*.js",
         "require": "./cjs/*.js"
      }
   },
   "scripts": {
      "start": "makepack start",
      "build": "makepack build",
      "build:publish": "makepack publish"
   },
   "devDependencies": {
      "@types/jest": "^29.0.0",
      "@types/react": "^19.0.2",
      "@types/react-dom": "^19.0.2",
      "jest": "^29.0.0",
      "makepack": "^1.5.5",
      "react": "^19.0.0",
      "react-dom": "^19.0.0",
      "typescript": "^4.4.2"
   },
   "keywords": [
      "debounce",
      "utility",
      "performance",
      "javascript",
      "typescript"
   ],
   "author": "",
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "https://github.com/devnax/dboun"
   },
   "homepage": "https://github.com/devnax/dboun#readme",
   "bugs": {
      "url": "https://github.com/devnax/dboun/issues"
   }
}